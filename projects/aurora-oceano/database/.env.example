# Aurora Oceano Database Config
# Copiar para .env e preencher

# Neon PostgreSQL (ou outro PostgreSQL)
DATABASE_URL=postgresql://user:password@host/database?sslmode=require

# Moloni API (copiar de /home/ubuntu/clawd/.env.secrets.moloni)
MOLONI_CLIENT_ID=auroroceanolda
MOLONI_CLIENT_SECRET=your_client_secret
MOLONI_EMPRESA_ID=276603
MOLONI_ACCESS_TOKEN=your_access_token
MOLONI_REFRESH_TOKEN=your_refresh_token
